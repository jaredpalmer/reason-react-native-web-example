{
  "name": "reason-react-native-web-example",
  "version": "0.6.3",
  "license": "MIT",
  "scripts": {
    "dev": "npm run js-watch | npm run bsb-watch",
    "js-watch": "razzle start",
    "bsb-watch": "bsb -make-world -w",
    "clean": "bsb -clean-world && rm -rf build",
    "build": "bsb -make-world && razzle build",
    "start": "NODE_ENV=production node build/server.js",
    "format": "find . -name \"*.re\" | grep -v \"node_modules\" | xargs bsrefmt --in-place"
  },
  "dependencies": {
    "bs-react-native-monorepo": "https://github.com/reasonml-community/bs-react-native.git",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "hpp": "^0.2.2",
    "morgan": "^1.9.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-native-web": "^0.11.4",
    "reason-react": "^0.7.0"
  },
  "devDependencies": {
    "bs-platform": "^5.0.4",
    "razzle": "^3.0.0"
  }
}
